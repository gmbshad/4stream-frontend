{
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "spaced-comment": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "no-trailing-spaces": 0,
    "vars-on-top": 0,
    "react/no-multi-comp": 0,
    "react/sort-comp": [2, {
      "order": [
        "static-methods",
        "lifecycle",
        "everything-else",
        "render"
      ]
    }]
  },
  "env": {
    "jest": true
  }
}
